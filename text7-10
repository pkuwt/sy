#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include<string.h>
int main() {
	int day = 0;
	scanf("%d", &day);
	printf("%d", day);
	switch (day) {
	case 1:
	case 2:
	case 3:
	case 4:
	case 5:
		printf("工作日");
		break;
	case 6:
	case 7:
		printf("周末");
		break;
	default:
		printf("输入错误");
		break;
	}
	//return 0;
	int i = 0;
	while (i <= 100) {
		if (i == 36) {
			i++;
			continue;
		}
		printf("%d\n", i);
		i++;
	}
	return 0;
}
